{
  "env": {
    "node": true
  },
  "rules": {
    "no-eval": 2,
    "no-console": 2,
    "strict": [2, "global"],
    "guard-for-in": 2,
    "wrap-iife": 2,
    "new-cap": 2,
    "no-caller": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-bitwise": 2,
    "no-empty": 2,
    "no-use-before-define": 2,
    "no-new": 2,
    "max-depth": [2, 3],
    "no-undef": 2,
    "no-unused-vars": 2,
    "max-params": [2, 4],
    "max-statements": [2, 20],
    "complexity": [2, 10],
    "quotes": [2, "single"],
    "no-mixed-requires": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "semi": 2
  }
}
